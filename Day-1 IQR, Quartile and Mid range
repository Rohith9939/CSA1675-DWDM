names = c("Frank", "John", "Kesav")
age<-c(23,21,22)
marks<-c(88,66,79)
df<-data.frame(names,age,marks)
IQR(df $age)
write.csv(df,"df.csv")

names = c("Frank", "John", "Kesav")
age<-c(23,21,22)
marks<-c(88,66,79)
df<-data.frame(names,age,marks)
quantile(df $age)
write.csv(df,"df.csv")

names = c("Frank", "John", "Kesav")
age<-c(23,21,22)
marks<-c(88,66,79)
df<-data.frame(names,age,marks)
range(df $age)
write.csv(df,"df.csv")
