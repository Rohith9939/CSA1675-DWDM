names = c("Frank", "John", "Kesav")
age<-c(23,21,22)
marks<-c(88,66,79)
df<-data.frame(names,age,marks)
mean(df $age)
write.csv(df,"df.csv")

names = c("Frank", "John", "Kesav")
age<-c(23,21,22)
marks<-c(88,66,79)
df<-data.frame(names,age,marks)
median(df $age)
write.csv(df,"df.csv")

names = c("Frank", "John", "Kesav")
age<-c(23,21,22)
marks<-c(88,66,79)
df<-data.frame(names,age,marks)
mode(df $age)
write.csv(df,"df.csv")
